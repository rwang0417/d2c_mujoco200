<mujoco model="lander6">
    <include file="./common/visual.xml"/>
    <include file="./common/skybox.xml"/>
    <include file="./common/materials.xml"/>
    
    <option timestep="0.01" iterations="50" solver="Newton" tolerance="1e-10" gravity="0 0 -9.8" collision="predefined" viscosity="0.0" integrator="Euler"/>

    <size njmax="200" nconmax="100" nstack="5000"/>

    <visual>
        <rgba haze=".15 .25 .35 1"/>
    </visual>

    <default>
        <joint type="free" damping="0.5"/>
        <motor ctrllimited="false" ctrlrange="-1 1" gear="1"/>
		<tendon stiffness="50" damping="4" width="0.04"/>
		<geom size="0.1" mass=".5" material="self"/>
		<site size="0.04"/>
    </default>

    <worldbody>
        <light directional="true" diffuse=".8 .8 .8" specular=".2 .2 .2" pos="0 0 5" dir="0 0 -1"/>

        <body pos="0 0 0">
            <geom name="bar1" type="capsule" fromto="1.5 0 0  1.5 2.6833 5.3666"/>
            <site name="b1s" pos="1.5 0 0"/>
			<site name="b1e" pos="1.5 2.6833 5.3666"/>
			<joint name="j1"/>
        </body>
        <body pos="0 0 0">
            <geom name="bar2" type="capsule" fromto="-1.5 0 0  -1.5 2.6833 5.3666"/>
            <site name="b2s" pos="-1.5 0 0"/>
			<site name="b2e" pos="-1.5 2.6833 5.3666"/>
			<joint name="j2"/>
        </body>
        <body pos="0 0 0">
            <geom name="bar3" type="capsule" fromto="0 -0.6708 5.3666  0 4.6957 2.6833"/>
            <site name="b3s" pos="0 -0.6708 5.3666"/>
			<site name="b3e" pos="0 4.6957 2.6833"/>
			<joint name="j3"/>
        </body>
        <body pos="0 0 0">
            <geom name="bar4" type="capsule" fromto="0 -2.0125 2.6833  0 3.3541 0"/>
            <site name="b4s" pos="0 -2.0125 2.6833"/>
			<site name="b4e" pos="0 3.3541 0"/>
			<joint name="j4"/>
        </body>
        <body pos="0 0 0">
            <geom name="bar5" type="capsule" fromto="3 0 3.3541  -3 0 3.3541"/>
            <site name="b5s" pos="3 0 3.3541"/>
			<site name="b5e" pos="-3 0 3.3541"/>
			<joint name="j5"/>
        </body>
        <body pos="0 0 0">
            <geom name="bar6" type="capsule" fromto="3 2.6833 2.0125  -3 2.6833 2.0125"/>
            <site name="b6s" pos="3 2.6833 2.0125"/>
			<site name="b6e" pos="-3 2.6833 2.0125"/>
			<joint name="j6"/>
        </body>
            
		<body pos="0 0 0">
			<geom name="floor" pos="0 0 -0.1" size="0 0 1" type="plane" material="grid"/>
			<site name="target" pos="3 1 4.5" size="0.12" rgba="1 0 0 .3"/>
		</body>
    </worldbody>

    <contact>
		<pair geom1="floor" geom2="bar1"/>
        <pair geom1="floor" geom2="bar2"/>
        <pair geom1="floor" geom2="bar3"/>
        <pair geom1="floor" geom2="bar4"/>
		<pair geom1="floor" geom2="bar5"/>
        <pair geom1="floor" geom2="bar6"/>
	</contact>

    <tendon>
        <spatial name="S1">
            <site site="b1e"/>
            <site site="b3s"/>
        </spatial>
		<spatial name="S2">
            <site site="b1e"/>
            <site site="b3e"/>
        </spatial>
        <spatial name="S3">
            <site site="b1e"/>
            <site site="b5s"/>
        </spatial>
        <spatial name="S4">
