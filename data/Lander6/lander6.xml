            <site site="b5e"/>
        </spatial>
        <spatial name="S16">
            <site site="b2s"/>
            <site site="b6e"/>
        </spatial>

        <spatial name="S17">
            <site site="b3s"/>
            <site site="b5s"/>
        </spatial>
		<spatial name="S18">
            <site site="b3s"/>
            <site site="b5e"/>
        </spatial>
        <spatial name="S19">
            <site site="b4s"/>
            <site site="b5s"/>
        </spatial>
        <spatial name="S20">
            <site site="b4s"/>
            <site site="b5e"/>
        </spatial>
		<spatial name="S21">
            <site site="b3e"/>
            <site site="b6s"/>
        </spatial>
        <spatial name="S22">
            <site site="b3e"/>
            <site site="b6e"/>
        </spatial>
        <spatial name="S23">
            <site site="b4e"/>
            <site site="b6s"/>
        </spatial>
		<spatial name="S24">
            <site site="b4e"/>
            <site site="b6e"/>
        </spatial> 
    </tendon>   

    <actuator>
        <motor tendon="S1"/>
        <motor tendon="S2"/>
		<motor tendon="S3"/>
        <motor tendon="S4"/>
        <motor tendon="S5"/>
        <motor tendon="S6"/>
		<motor tendon="S7"/>
        <motor tendon="S8"/>
        <motor tendon="S9"/>
        <motor tendon="S10"/>
        <motor tendon="S11"/>
        <motor tendon="S12"/>
		<motor tendon="S13"/>
        <motor tendon="S14"/>
        <motor tendon="S15"/>
        <motor tendon="S16"/>
		<motor tendon="S17"/>
        <motor tendon="S18"/>
        <motor tendon="S19"/>
        <motor tendon="S20"/>
		<motor tendon="S21"/>
        <motor tendon="S22"/>
		<motor tendon="S23"/>
        <motor tendon="S24"/>
    </actuator>
	
	<sensor>
		<framelinvel objtype="site" objname="b1s"/>
		<framelinvel objtype="site" objname="b2s"/>
		<framelinvel objtype="site" objname="b3s"/>
		<framelinvel objtype="site" objname="b4s"/>
		<framelinvel objtype="site" objname="b5s"/>
		<framelinvel objtype="site" objname="b6s"/>
		
		<framelinvel objtype="site" objname="b1e"/>
		<framelinvel objtype="site" objname="b2e"/>
        <framelinvel objtype="site" objname="b3e"/>
		<framelinvel objtype="site" objname="b4e"/>
		<framelinvel objtype="site" objname="b5e"/>
        <framelinvel objtype="site" objname="b6e"/>
	</sensor>
</mujoco>
